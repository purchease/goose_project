<style>
    .square {
        height: 100px;
        width: 100px;
        background: url("https://i.ibb.co/8K0tcGR/image-11.png");
        display: flex;
        align-items: center;
        justify-content: center;
        background-size: 100px;
    }
    .square_bump {
        height: 100px;
        width: 100px;
        background-image: url("https://i.ibb.co/PGYX9Vq/image-12.png");
        display: flex;
        align-items: center;
        justify-content: center;
        background-size: 100px;
    }
    .square_move_back_3_cases {
        height: 100px;
        width: 100px;
        background-image: url("https://i.ibb.co/51Bhvxy/image-14.png");
        display: flex;
        align-items: center;
        justify-content: center;
        background-size: 100px;
    }
    .square_two_times {
        height: 100px;
        width: 100px;
        background-image: url("https://i.ibb.co/Ryt0LZk/image-13.png");
        display: flex;
        align-items: center;
        justify-content: center;
        background-size: 100px;
    }
    .square_hole {
        height: 100px;
        width: 100px;
        background-image: url("https://i.ibb.co/RPLtjqX/image-11.png");
        display: flex;
        align-items: center;
        justify-content: center;
        background-size: 100px;
    }

    .number_position {
        flex-direction: column;
        align-self: flex-start;
    }
    .logo_for_special_cases {
        flex-direction: column;
        align-self: flex-end
    }
</style>


<div class="<%= display_icon(space.space_skill_id) %>">
  <div class="number_position">
    <%= space.position %>
  </div>
  <% unless @positions.empty? %>
    <% @positions.each do |position| %>
    <div class="">
      <% if position.space == space %>
        <%#= display_oie(@positions.first.player.color) %>
        <%= position.player.color %>
      <% end %>
    </div>
    <% end %>
  <% end  %>
</div>
